-- Simple ATL query for testing basic expression evaluation
-- This file tests basic literal expressions and arithmetic operations
module SimpleQuery;

query SimpleAddition = 10 + 5;

query SimpleSubtraction = 20 - 8;

query SimpleMultiplication = 6 * 7;

query SimpleDivision = 100 / 4;

query StringLiteral = 'Hello World';

query BooleanTrue = true;

query BooleanFalse = false;

query ComparisonEqual = 5 = 5;

query ComparisonNotEqual = 3 <> 7;

query LogicalAnd = true and false;

query LogicalOr = true or false;
